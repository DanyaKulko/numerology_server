        .top-bar, .bottom-bar {
            position: fixed;
            left: -5px;
            width: calc(100% + 10px);
            height: var(--bar-h);
            background: var(--bar-color);
            z-index: 10;
        }

        .top-bar {
            top: 0;
        }

        .bottom-bar {
            bottom: -1px;
