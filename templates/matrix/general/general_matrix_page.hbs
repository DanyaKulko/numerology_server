        .shema .circle {
            position: absolute;
            width: fit-content;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .shema .number {
            width: calc(8.4px * var(--scale));
            position: absolute;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: calc(6px * var(--scale));
        }

        .circle {
            --scale: 1;
        }

        .circle.small {
            height: calc(16px * var(--scale));
            width: calc(10.4px * var(--scale));
            font-size: calc(8.8px * var(--scale));
            padding-bottom: calc(1.6px * var(--scale));
            font-weight: bold;
        }

        .circle.medium {
            height: calc(20px * var(--scale));
            width: calc(14.16px * var(--scale));
            font-size: calc(12px * var(--scale));
            font-weight: bold;
        }

        .circle.mediumPlus {
            height: calc(23.2px * var(--scale));
            font-size: calc(12.8px * var(--scale));
        }

        .circle.large {
            width: calc(18.88px * var(--scale));
            font-size: calc(16px * var(--scale));
            color: #fff;
            font-weight: 700;
