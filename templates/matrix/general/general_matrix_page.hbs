        .calculator {
            background: #fff;
            width: 313px;
            border-radius: 24px;
            overflow: hidden;
            position: relative;
            z-index: 1;
        }

        .calculator__header {
            background: linear-gradient(270deg, #145833 0%, #207146 100%);
            color: #ffffff;
            text-align: center;
            padding: 17px 5px;
            font-size: 15px;
            font-weight: 700;
            letter-spacing: 0.05em;
            line-height: 100%;
        }

        .calculator__table {
            width: 100%;
            padding: 20px;
            border-spacing: 0;
            border-collapse: collapse;
            background-color: transparent;
            position: relative;
        }

        .calculator__table thead th {
            color: rgba(29, 97, 59, 1);
            padding: 16px 0;
            text-align: center;
            font-weight: 700;
            font-size: 10px;
            line-height: 100%;
            vertical-align: middle;
        }

        .calculator__table th,
        .calculator__table td,
        .calculator__table thead,
        .calculator__table tbody,
        .calculator__table tfoot,
        .calculator__table tr {
            /*background-color: #fff;*/
            background-color: transparent !important;

        }

        .calculator__table thead th:first-child {
            text-align: center;
            padding-left: 7px;
        }

        .calculator__table tbody td,
        .calculator__table tfoot td {
            text-align: center;
            font-weight: 700;
            font-size: 16px;
            line-height: 100%;
            vertical-align: middle;
            color: rgba(18, 159, 64, 1);
        }

        .calculator__table tbody td.chakra {
            text-align: left;
            font-size: 16px;
            font-weight: 500;
            color: var(--text-dark);
        }

        .chakra-cell {
            display: flex;
            flex-direction: column;
            align-items: center;
            padding-bottom: 5px;
            gap: 2px;
        }

        .chakra-cell img {
            width: 27px;
            height: 27px;
            object-fit: contain;
            flex-shrink: 0;
        }

        .chakra-cell span {
            color: var(--text-dark);
            font-weight: 500;
            font-size: 10px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
        }


        .calculator__table tfoot td {
            font-size: 18px;
            font-weight: 700;
        }

        .calculator__table tfoot td.total {
            color: var(--green-dark);
            padding: 16px 0;
        }

        .calculator__table tfoot td:first-child {
            text-align: center;
            padding-left: 10px;
            color: rgba(29, 97, 59, 1);
        }

        .chakra_number {
            border-bottom: 1px solid rgba(29, 97, 59, 1);
        }


        .aspect-card {
            border-radius: 20px;
            width: 263px;
            box-shadow: 0px 0px 4px 0px #00000040;
        }

        .aspect-card__firstHeader {
            font-weight: 700;
            font-size: 17px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
            text-transform: uppercase;
            border-top-left-radius: 20px;
            border-top-right-radius: 20px;
            background: linear-gradient(270deg, #145833 0%, #207045 100%);
            color: rgba(255, 255, 255, 1);
            padding: 17px;
            width: 100%;
        }

        .aspect-card__firstBody {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 10px;
            justify-content: center;
            font-weight: 600;
            font-size: 12px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
            padding: 10px;
            background: rgba(249, 249, 247, 1);
            color: rgba(29, 97, 59, 1);
        }

        .aspect-card__triad {
            position: relative;
        }

        .aspect-card__triad img {
            width: 237px;
        }

        .aspect-card__number {
            font-weight: 700;
            font-size: 24px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
            color: rgba(249, 249, 247, 1);
            width: 28.3px;
        }

        .aspect-card .r,
        .aspect-card .t {
            position: absolute;
            top: 30px;
            left: 8px;
        }

        .aspect-card .s,
        .aspect-card .u {
            position: absolute;
            top: 10px;
            left: 105px;
        }

        .aspect-card .y,
        .aspect-card .v {
            position: absolute;
            top: 30px;
            right: 8px;
        }

        .aspect-card__firstBody p {
            font-weight: 700;
            font-size: 16px;
            line-height: 100%;
            color: rgba(29, 97, 59, 1);
        }

        .aspect-card__secondHeader {
            padding: 13px;
            width: 100%;
            font-weight: 600;
            font-size: 15px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
            text-transform: uppercase;
            background: linear-gradient(270deg, #145833 0%, #207045 100%);
            color: rgba(255, 255, 255, 1);
        }

        .aspect-card__secondBody {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 10px;
            justify-content: center;
            position: relative;
            padding: 10px 10px 30px 10px;
            width: 100%;
            background: rgba(249, 249, 247, 1);
            color: rgba(29, 97, 59, 1);
            font-weight: 600;
            font-size: 12px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
            border-bottom-left-radius: 20px;
            border-bottom-right-radius: 20px;
        }

        .aspect-card__secondBody p {
            font-weight: 700;
            font-size: 16px;
            line-height: 100%;
            color: rgba(29, 97, 59, 1);
        }

        .page-container {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            height: 100vh;
            width: 100%;
            gap: 20px;
        }

        .data-container {
            display: flex;
            padding: 0 40px;
            gap: 20px;
            justify-content: space-between;
            align-items: center;
        }

        .firstSection__matrixForm {
            display: flex;
            gap: 10px;
            justify-content: center;
            align-items: center;
        }

        .firstSection__formItem {
            width: 108px;
            /*height: 65px;*/
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            gap: 10px;
        }

        .firstSection__label {
            font-weight: 700;
            font-size: 18px;
            line-height: 100%;
            text-align: center;
            vertical-align: middle;
            color: rgba(29, 97, 59, 1);
        }

        .pill_container {
            width: 80px;
            position: relative;
        }
        .pill {
            width: 100%;
        }
        .pill_value {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: #422722;
            font-size: 16px;
            font-weight: 700;
        }
    </style>

</head>
