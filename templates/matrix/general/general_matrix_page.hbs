                <div class="year5 number">{{numbers.a6_7}}</div>
                <div class="year6 number">{{numbers.a7_8}}</div>
                <div class="year7 number">{{numbers.a8_9}}</div>

                <div class="year11 number">{{numbers.f11_12}}</div>
                <div class="year12 number">{{numbers.f12_13}}</div>
                <div class="year13 number">{{numbers.f13_14}}</div>
                <div class="year14 number">{{numbers.f15}}</div>
                <div class="year15 number">{{numbers.f16_17}}</div>
                <div class="year16 number">{{numbers.f17_18}}</div>
                <div class="year17 number">{{numbers.f18_19}}</div>
            </div>
        </div>

        <div class="calculator">
            <div class="calculator__header">{{translations.calculator.title}}</div>

            <table class="calculator__table">
                <thead>
                <tr>
                    <th>{{translations.calculator.thead.chakra}}</th>
                    <th>{{translations.calculator.thead.physical}}</th>
                    <th>{{translations.calculator.thead.energy}}</th>
                    <th>{{translations.calculator.thead.mental}}</th>
                </tr>
                </thead>

                <tbody class="calculator__body">
                {{#each chakraRows}}
                    <tr>
                        <td class="chakra">
                            <div class="chakra-cell">
                                <img src="{{this.chakra.icon}}" alt="chakra {{@index}}" class="chakra-icon" width="27"
                                     height="27"/>
                                <span>{{this.chakra.name}}</span>
                            </div>
                        </td>
                        <td class="chakra_number">{{this.o}}</td>
                        <td class="chakra_number">{{this.p}}</td>
                        <td class="chakra_number">{{this.q}}</td>
                    </tr>
                {{/each}}
                </tbody>

                <tfoot>
                <tr class="totalRow">
                    <td class="total">{{translations.calculator.total}}</td>
                    <td class="total">{{numbers.o}}</td>
                    <td class="total">{{numbers.p}}</td>
                    <td class="total">{{numbers.q}}</td>
                </tr>
                </tfoot>
            </table>
        </div>
    </div>
    <div class="firstSection__matrixForm">
        <div class="firstSection__formItem">
            <label for="day" class="firstSection__label">{{translations.day}}</label>
            <div class="pill_container">
                <img src="{{pill}}" class="pill" alt="">
                <span class="pill_value">
                    {{numbers.day}}
                </span>
            </div>
        </div>
        <div class="firstSection__formItem">
            <label for="month" class="firstSection__label">{{translations.month}}</label>
            <div class="pill_container">
                <img src="{{pill}}" class="pill" alt="">
                <span class="pill_value">
                    {{numbers.month}}
                </span>
            </div>
        </div>
        <div class="firstSection__formItem">
            <label for="year" class="firstSection__label">{{translations.year}}</label>
            <div class="pill_container">
                <img src="{{pill}}" class="pill" alt="">
                <span class="pill_value">
                    {{numbers.year}}
                </span>
            </div>
        </div>
    </div>
</div>
</body>
</html>
